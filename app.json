{
    "expo": {
        "name": "Lanpay",
        "slug": "Lanpay",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "newArchEnabled": true,
        "scheme": "lanpay",
        "splash": {
            "image": "./assets/splash-icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#EFEFF4"
        },
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#EFEFF4"
            }
        },
        "web": {
            "bundler": "metro",
            "favicon": "./assets/favicon.png"
        },
        "plugins": [
            "expo-router"
        ]
    }
}
